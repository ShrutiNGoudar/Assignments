
public class StaticVariableExampleFour {

    static String vesselName = "Pegion";
    static String vessecolor = "Silver";
    static short vesselDiameter = 240;
    static int vesselPrice = 500;
    static boolean vesselIsUsable = true;

    public static void main(String[] args) {
        System.out.println(vesselName);
        System.out.println(vessecolor);
        System.out.println(vesselDiameter);
        System.out.println(vesselPrice);
        System.out.println(vesselIsUsable);

    }
}










